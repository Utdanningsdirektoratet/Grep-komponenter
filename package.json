{
  "name": "grep-components",
  "version": "0.18.5-grepf-1393.2",
  "description": "Grep komponentbibliotek",
  "repository": {
    "type": "git",
    "url": "https://github.com/Utdanningsdirektoratet/Grep-komponenter.git"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "type": "module",
  "homepage": "https://github.com/Utdanningsdirektoratet/Grep-komponenter",
  "author": "Udir",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "pretest": "tsc --noEmit",
    "test": "jest",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "yalc:init": "yalc add && yalc push",
    "yalc:build": "rollup -c --no-treeshake --no-sourcemap --environment yalc",
    "yalc:watch": "npm run yalc:build -- -w",
    "prepare": "husky install"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps*",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "feature/*",
        "channel": "${name.replace(/^feature\\//g, \"\")}",
        "prerelease": "${name.replace(/^feature\\//g, \"\")}"
      }
    ]
  },
  "peerDependencies": {
    "@date-io/dayjs": "^1.3.13",
