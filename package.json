    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-jest": "^29.2.0",
    "babel-loader": "^9.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-runtime": "^6.26.0",
